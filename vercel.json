{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/change-detection",
      "schedule": "*/2 * * * *"
    },
    {
      "path": "/api/cron/reconciliation",
      "schedule": "0 0 * * *"
    }
  ],
  "env": {
    "PYTHONUNBUFFERED": "1"
  }
}

