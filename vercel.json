{
  "version": 2,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/index"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/change-detection",
      "schedule": "*/2 * * * *"
    },
    {
      "path": "/api/cron/reconciliation",
      "schedule": "0 0 * * *"
    }
  ]
}

