{
  "version": 2,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/index"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/change-detection",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/api/cron/full-sync",
      "schedule": "0 2 */3 * *"
    }
  ]
}

